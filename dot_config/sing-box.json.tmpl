{
  "log": {
    "level": "info"
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
{{- if eq .chezmoi.os "linux" }}
      "interface_name": "singtun0",
      "stack": "system",
      "mtu": 1500,
{{- end }}
      "address": [
        "172.19.0.1/30"
      ],
      "auto_route": true,
      "strict_route": false
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct-out"
    },
    {
      "type": "vless",
      "tag": "proxy-out",
      "server": "{{ .singbox.server }}",
      "server_port": 443,
      "uuid": "{{ .singbox.uuid }}",
      "flow": "xtls-rprx-vision",
      "tls": {
        "enabled": true,
        "reality": {
          "enabled": true,
          "public_key": "{{ .singbox.public_key }}",
          "short_id": "{{ .singbox.short_id }}"
        },
        "server_name": "marktplaats.nl",
        "utls": {
          "enabled": true,
          "fingerprint": "firefox"
        }
      }
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "rule_set": [
      {
        "tag": "cloudflare-ips",
        "type": "local",
        "format": "source",
        "path": "{{ .chezmoi.homeDir }}/.config/sing-box-cloudflare-ip-ruleset.json"
      },
      {
        "tag": "proxy-domains",
        "type": "local",
        "format": "source",
        "path": "{{ .chezmoi.homeDir }}/.config/sing-box-domain-ruleset.json"
      },
      {
        "tag": "proxy-ips",
        "type": "local",
        "format": "source",
        "path": "{{ .chezmoi.homeDir }}/.config/sing-box-ip-ruleset.json"
      }
    ],
    "rules": [
      {
        "action": "sniff"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "rule_set": [ "proxy-domains" ],
        "action": "route",
        "outbound": "proxy-out"
      },
      {
        "rule_set": [ "cloudflare-ips" ],
        "action": "route",
        "outbound": "proxy-out"
      },
      {
        "rule_set": [ "proxy-ips" ],
        "action": "route",
        "outbound": "proxy-out"
      },
      {
        "process_name": "Discord",
        "action": "route",
        "outbound": "proxy-out"
      },
{{- if eq .chezmoi.os "darwin" }}
      {
        "process_name": "Discord Helper",
        "action": "route",
        "outbound": "proxy-out"
      },
      {
        "process_name": "Discord Helper (Plugin)",
        "action": "route",
        "outbound": "proxy-out"
      },
      {
        "process_name": "Discord Helper (Renderer)",
        "action": "route",
        "outbound": "proxy-out"
      },
      {
        "process_name": "Discord Helper (GPU)",
        "action": "route",
        "outbound": "proxy-out"
      },
      {
        "process_name": "AnyDesk",
        "action": "route",
        "outbound": "proxy-out"
      },
      {
        "process_name": "RustDesk",
        "action": "route",
        "outbound": "proxy-out"
      },
{{- end }}
      {
        "process_name": "EliteDangerous64.exe",
        "action": "route",
        "outbound": "proxy-out"
      },
{{- if eq .chezmoi.os "linux" }}
      {
        "process_name": "Tabletop Simulator.x86_64",
        "action": "route",
        "outbound": "proxy-out"
      },
      {
        "process_name": "Telegram",
        "action": "route",
        "outbound": "proxy-out"
      },
{{- end }}
      {
        "process_name": "TETR.IO",
        "action": "route",
        "outbound": "proxy-out"
      },
      {
        "action": "route",
        "outbound": "direct-out"
      }
    ]
  },
  "dns": {
    "servers": [
      {
        "tag": "dns-direct",
        "type": "udp",
        "server": "8.8.8.8"
      },
      {
        "tag": "dns-proxy",
        "type": "udp",
        "server": "1.1.1.1",
        "detour": "proxy-out"
      }
    ],
    "strategy": "prefer_ipv4",
    "rules": [
      {
        "rule_set": "proxy-domains",
        "action": "route",
        "server": "dns-proxy"
      },
      {
        "action": "route",
        "server": "dns-direct"
      }
    ]
  }
}
